{
  "name": "adobe-ide",
  "version": "0.0.2",
  "cep": {
    "name": "Adobe IDE",
    "id": "com.aet.adobeide",
    "hosts": "*",
    "debugInProduction": true
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "watch:extendscript": "webpack --config extendscript.config.js --watch",
    "watch:cep": "webpack-dev-server --config cep.config.js",
    "start": "export IS_DEV=1 && npm-run-all --parallel watch:**",
    "build:extendscript": "webpack --config extendscript.config.js --display verbose",
    "build:jsxbin": "node ./vendor/exportToJSX.js -n ./dist/extendscript.js || true",
    "build:cep": "webpack --config cep.config.js --display verbose && rm dist/extendscript.js",
    "build": "npm-run-all --serial build:**",
    "ci:extendscript": "webpack --config extendscript.config.js --display verbose",
    "ci:cep": "webpack --config cep.config.js --display verbose",
    "ci": "export IS_DEV=1 && npm-run-all --serial ci:** installers",
    "zxp": "create-zxp ./dist",
    "installers": "cep-packager ./dist",
    "build-zxp": "npm-run-all build zxp",
    "build-installers": "npm-run-all build installers"
  },
  "dependencies": {
    "fs-extra": "^6.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.2",
    "@types/node": "^10.12.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "cep-bundler-webpack": "0.0.28",
    "cep-interface": "^0.4.2",
    "cep-packager": "0.0.25",
    "console-feed": "^2.8.6",
    "create-zxp": "^0.1.0",
    "extendscript-es5-shim-ts": "0.0.1",
    "monaco-editor": "^0.16.2",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "rimraf": "^2.6.2",
    "types-for-adobe": "github:pravdomil/types-for-adobe",
    "typescript": "^3.4.1",
    "write-file-webpack-plugin": "^4.5.0"
  }
}
